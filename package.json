{
  "name": "vscode-extension-eval",
  "displayName": "vscode-extension-eval",
  "description": "run wapper command of other extensions",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-extension-eval.action"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-extension-eval.action",
        "title": "Eval Code from arg (call from keybindings.json)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "vscode-test": "^1.4.1"
  },
  "dependencies": {
    "typescript": "^4.1.2"
  },
  "publisher": "umaumax",
  "repository": {
    "type": "git",
    "url": "https://github.com/umaumax/vscode-extension-eval.git"
  }
}
